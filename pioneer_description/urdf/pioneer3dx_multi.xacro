  </link>

<!-- Fork Vertical Base -->
  <joint name="fork_vertical_base_joint" type="fixed">
    <origin xyz="0.245 0 0.124" rpy="0 -0.01 0"/>
    <parent link="top_plate"/>
    <child link="fork_vertical_base"/>
  </joint>
  
  <link name="fork_vertical_base">
