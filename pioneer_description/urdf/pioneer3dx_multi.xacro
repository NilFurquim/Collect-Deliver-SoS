		</collision>
	</link>

	<joint name="left_wheel_hub_joint" type="fixed">
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<parent link="left_wheel"/>
		<child link="left_hub"/>
	</joint>

	<link name="left_hub">
		<visual>
