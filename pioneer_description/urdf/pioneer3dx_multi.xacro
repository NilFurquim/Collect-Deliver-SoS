      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	      <box size="0.30 0.0398 0.0398"/>
      </geometry>
     <material name="CameraGreen"/>
    </visual>

    <inertial>
      <mass value="1e-4" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
